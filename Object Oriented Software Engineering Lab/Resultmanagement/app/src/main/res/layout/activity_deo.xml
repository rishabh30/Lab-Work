<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.rj.android.resultmanagement.user.DEO">



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Change Password"
        android:onClick="changePass"
        android:id="@+id/changePas"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="36dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LOGGED IN AS DEO"
        android:id="@+id/textView9"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update Result"
        android:onClick="updateMarks"
        android:id="@+id/updateMarks"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/view"
        android:layout_alignParentStart="true" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOG OUT"
        android:onClick="logout"
        android:id="@+id/logout"
        android:layout_alignTop="@+id/changePas"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD "
        android:onClick="add"
        android:id="@+id/add"
        android:layout_marginTop="89dp"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UPDATE"
        android:onClick="update"
        android:id="@+id/update"
        android:layout_alignTop="@+id/add"
        android:layout_toEndOf="@+id/updateMarks" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DELETE"
        android:onClick="delete"
        android:id="@+id/delete"
        android:layout_alignTop="@+id/update"
        android:layout_alignStart="@+id/logout" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VIEW"
        android:onClick="view"
        android:id="@+id/view"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/update" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD RESULT"
        android:onClick="addResult"
        android:id="@+id/addResult"
        android:layout_alignBottom="@+id/updateMarks"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
